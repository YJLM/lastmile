<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
<meta name="description" content="<%= t 'app.description' %>" />
<meta name="keywords" content="<%= t 'app.tags' %>" />
<meta property="og:title" content="<%= t 'app.title' %>" />
<meta property="og:type" content="website"/>
<meta property="description" content="<%= t 'app.description' %>" />
<meta property="og:image" content="<%= image_url 'social/social.png', only_path: false %>" />
<meta property="og:image:type" content="image/png" />
<meta property="og:image:width" content="200" />
<meta property="og:image:height" content="200" />
<meta property="og:site_name" content="<%= I18n.t('app.title') %>" />
<%= favicon_link_tag '/favicon.ico', rel: 'icon' %>
<%= favicon_link_tag '/favicon.ico', rel: 'shortcut icon' %>